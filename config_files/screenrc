# Encoding
defencodig utf-8

# Prefix Key
escape ^Jj

# set scrollback
defscrollback 10000

# è‰²
defbce on
term xterm-256color

# Delete sartt up screen
startup_message off

# Enable Auto detach
autodetach on

#
altscreen on

# Show date and screen list
hardstatus alwayslastline "%{= cd} %-w%{= wk} %n %t* %{-}%+w %= LoadAVG [%l] "
